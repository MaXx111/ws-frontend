{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
          ],
          "import/extensions": [
            "error",
            "ignorePackages"
            ],
            "import/no-unresolved": 0,
            "no-console": 0,
            "class-methods-use-this": 0
    }
}